}

class QuickFindUF {
	private int[] id;
	
	public QuickFindUF(int N){
		// initialization of the component set
		id = new int[N];
		for(int i:id){
			id[i] = i;
		}
	}
	
	public void union(int p, int q){
		
	}
	
	public boolean connected(int p, int q){
		return (id[p] == id[q]);
	}
}
