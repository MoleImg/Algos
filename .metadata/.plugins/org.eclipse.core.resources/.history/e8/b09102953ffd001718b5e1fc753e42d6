package fundModels.Sort;

public class Selection extends SortAlgos{

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	@Override
	public void sort(Comparable[] a) {
		int N = a.length;
		for (int i = 0; i < N; i++) {
			int min = i;
			for (int j = i; j < N; j++) {
				if (less(a[j], a[i])) swap(a, i, j) 
			}
		}
		
	}

}
