package fundModels;

public class ResizingArrayStackOfStrings {
	private String[] s;
	private int N = 0;
	
	public ResizingArrayStackOfStrings() {
		s = new String[1];
	}

	public boolean isEmpty() {
		return N == 0;
	}
	
	public void push(String item) {
		if (N == s.length) resize(2 * s.length);
		s[N++] = item;
	}
	
